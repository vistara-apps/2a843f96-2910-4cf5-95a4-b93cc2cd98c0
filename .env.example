# OnchainKit API Key (get from Coinbase Developer Platform)
NEXT_PUBLIC_ONCHAINKIT_API_KEY=your_onchainkit_api_key_here

# WalletConnect Project ID (get from WalletConnect Cloud)
NEXT_PUBLIC_WC_PROJECT_ID=your_walletconnect_project_id_here

# x402 Protocol Endpoint (optional - for micropayments)
NEXT_PUBLIC_X402_ENDPOINT=https://your-x402-endpoint.com

# Base Network RPC URL (optional - uses default if not provided)
NEXT_PUBLIC_BASE_RPC_URL=https://mainnet.base.org