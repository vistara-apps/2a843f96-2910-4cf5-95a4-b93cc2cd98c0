# OnchainKit API Key (get from https://portal.cdp.coinbase.com/)
NEXT_PUBLIC_ONCHAINKIT_API_KEY=your_api_key_here

# Base RPC URL (optional, OnchainKit provides default)
NEXT_PUBLIC_BASE_RPC_URL=https://mainnet.base.org

# Farcaster Hub API (optional)
NEXT_PUBLIC_FARCASTER_HUB_URL=https://hub.farcaster.xyz

# OpenAI API Key (for AI gift suggestions)
OPENAI_API_KEY=your_openai_key_here

# Upstash Redis (for caching)
UPSTASH_REDIS_REST_URL=your_upstash_url
UPSTASH_REDIS_REST_TOKEN=your_upstash_token
