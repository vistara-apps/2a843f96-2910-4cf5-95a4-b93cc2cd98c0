# OnchainKit API Key (get from https://portal.cdp.coinbase.com/)
NEXT_PUBLIC_ONCHAINKIT_API_KEY=your_api_key_here

# Base Network RPC (optional, OnchainKit provides default)
NEXT_PUBLIC_BASE_RPC_URL=https://mainnet.base.org

# OpenAI API Key (for AI gift suggestions)
OPENAI_API_KEY=your_openai_key_here

# Upstash Redis (for caching and session management)
UPSTASH_REDIS_REST_URL=your_upstash_url_here
UPSTASH_REDIS_REST_TOKEN=your_upstash_token_here

# Printful API (for physical gift fulfillment)
PRINTFUL_API_KEY=your_printful_key_here

# Pinata/IPFS (for video message storage)
PINATA_API_KEY=your_pinata_key_here
PINATA_SECRET_KEY=your_pinata_secret_here
